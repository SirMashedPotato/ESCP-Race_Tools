<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ========== [ESCP_NonPlayerFactionBase_Tribal] ========== -->
	<FactionDef ParentName="TribeBase" Name="ESCP_NonPlayerFactionBase_Tribal" Abstract="True">
		<caravanTraderKinds>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.MedievalModule">VFEM_Caravan_Medieval_Farmer</li>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.MedievalModule">VFEM_Caravan_Medieval_Armourer</li>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.MedievalModule">VFEM_Caravan_Medieval_Weaponsmith</li>
		</caravanTraderKinds>
		<visitorTraderKinds>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.MedievalModule">VFEM_Visitor_Medieval_Standard</li>
		</visitorTraderKinds>
		<baseTraderKinds>
			<li MayRequire="OskarPotocki.VanillaFactionsExpanded.MedievalModule">VFEM_Base_Medieval_Standard</li>
		</baseTraderKinds>
		<xenotypeSet>
			<xenotypeChances>
				<Sanguophage MayRequire="Ludeon.RimWorld.Biotech">0.001</Sanguophage>
			</xenotypeChances>
		</xenotypeSet>
		<pawnGroupMakers Inherit="false"/>
		<maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
	</FactionDef>
	<!-- ========== [ESCP_NonPlayerFactionBase_TribalCivil] ========== -->
	<FactionDef ParentName="ESCP_NonPlayerFactionBase_Tribal" Name="ESCP_NonPlayerFactionBase_TribalCivil" Abstract="True">
		<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Supremacist</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Raider</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
		</disallowedMemes>
		<configurationListOrderPriority>30</configurationListOrderPriority>
	</FactionDef>
	<!-- ========== [ESCP_NonPlayerFactionBase_TribalRough] ========== -->
	<FactionDef ParentName="ESCP_NonPlayerFactionBase_Tribal" Name="ESCP_NonPlayerFactionBase_TribalRough" Abstract="True">
		<naturalEnemy>true</naturalEnemy>
		<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Supremacist</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
		</disallowedMemes>
		<configurationListOrderPriority>40</configurationListOrderPriority>
	</FactionDef>
	<!-- ========== [ESCP_NonPlayerFactionBase_TribalSavage] ========== -->
	<FactionDef ParentName="ESCP_NonPlayerFactionBase_Tribal" Name="ESCP_NonPlayerFactionBase_TribalSavage" Abstract="True">
		<permanentEnemy>true</permanentEnemy>
		<requiredMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Supremacist</li>
		</requiredMemes>
		<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
		</disallowedMemes>
		<configurationListOrderPriority>50</configurationListOrderPriority>
	</FactionDef>
</Defs>
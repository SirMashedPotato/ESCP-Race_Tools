<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[ESCP_GenericBackstoryAdulthood_Base]========== -->
	<!-- 
		Should only have generic adulthood backstories, as childhood ones may cause issues with meditation types 
	-->
	<AlienRace.AlienBackstoryDef Name="ESCP_GenericBackstoryAdulthood_Base" Abstract="True">
		<slot>Adulthood</slot>
		<spawnCategories>
			<li>ESCP_GenericBackstory_AdulthoodMedieval</li>
		</spawnCategories>
	</AlienRace.AlienBackstoryDef>


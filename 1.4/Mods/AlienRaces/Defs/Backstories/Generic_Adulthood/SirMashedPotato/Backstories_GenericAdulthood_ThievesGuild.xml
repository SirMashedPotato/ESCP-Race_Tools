<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[ESCP_GenericBackstory_Adulthood_ThievesGuildEar]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_GenericBackstoryAdulthood_ThievesGuildBase">
		<defName>ESCP_GenericBackstory_Adulthood_ThievesGuildEar</defName>
		<title>Thieves Guild ear</title>
		<titleShort>guild ear</titleShort>
		<baseDesc>While not exactly a member of the Thieves Guild, [PAWN_nameDef] did know someone who knew someone, and ensured that someone got plentiful information on potential hauls, all for a generous cut of course.</baseDesc>
		<bodyTypeGlobal>Thin</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Intellectual</key>
				<value>3</value>
			</li>
			<li>
				<key>Social</key>
				<value>3</value>
			</li>
		</skillGains>
		<forcedTraitsChance>
			<li>
				<defName>Greedy</defName>
				<chance>15</chance>
			</li>
		</forcedTraitsChance>
		<maleCommonality>90</maleCommonality>
		<femaleCommonality>90</femaleCommonality>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_GenericBackstory_Adulthood_ThievesGuildFence]========== -->

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- The Eight -->

	<MemeDef>
		<defName>ESCP_Structure_OriginDivinesEight</defName>
		<label>Eight Divines origin</label>
		<description>This is a variant of the abstract theist structure. This ideoligion is a descendant of the Temple of the Divines, a religious sect focused on worshipping the eight Divines, Aedric beings of creation. It may have very different precepts than those of thousands of years ago, but its symbols are similar, though the exact names of it's deities, and the roles they play, are known to vary between cultures.</description>
		<modExtensions>
			<li Class="ESCP_RaceTools.IdeoOrginProperties">
				<IgnoreDuplicateDeityNames>true</IgnoreDuplicateDeityNames>
				<overrideDeityIcons>true</overrideDeityIcons>
				<iconPath>
					<li>UI/Deities/ESCP_DeityAkatosh</li>
					<li>UI/Deities/ESCP_DeityArkay</li>
					<li>UI/Deities/ESCP_DeityDibella</li>
					<li>UI/Deities/ESCP_DeityJulianos</li>
					<li>UI/Deities/ESCP_DeityKynareth</li>
					<li>UI/Deities/ESCP_DeityMara</li>
					<li>UI/Deities/ESCP_DeityStendarr</li>
					<li>UI/Deities/ESCP_DeityZenithar</li>
				</iconPath>
            </li>
		</modExtensions>
		<iconPath>UI/Structures/ESCP_OriginDivinesEight</iconPath>
		<deityCount>8</deityCount>
		<allowSymbolsFromDeity>false</allowSymbolsFromDeity>
		<symbolPackOverride>true</symbolPackOverride>
		<worshipRoomLabel>temple</worshipRoomLabel>
		<category>Structure</category>
		<impact>0</impact>
		<requiredRituals>
		</requiredRituals>
		<consumableBuildings>
		</consumableBuildings>
		<requireAnyRitualSeat>
		</requireAnyRitualSeat>
		<generalRules>
		  <rulesStrings>
			<li>memeCreed->sect</li>
			<li>memeCreed->cult</li>
			<li>memeCreed->temple</li>
			<li>memeCreed->faith</li>
			<li>memeLeaderNoun->patriarch</li>
			<li>memeLeaderAdjective->chosen</li>
			<li>memeLeaderAdjective->divine</li>
			<li>memeLeaderAdjective->blessed</li>
			<li>memeMoralist->priest</li>
			<li>memeMoralist->cleric</li>
			<li>memeMoralist->preacher</li>
		  </rulesStrings>
		</generalRules>
	   <descriptionMaker>
		 <patterns>
		   <li><def>TheistMyth</def></li>
		   <li><def>TheistFounder</def></li>
		 </patterns>
		 <constants>
		   <li><key>structure_theist</key><value>true</value></li>
		   <li><key>ESCP_Structure_OriginDivinesEight</key><value>true</value></li>
		 </constants>
	   </descriptionMaker>
	   <symbolPacks>
		  <li><ideoName>Temple of the Divines</ideoName><adjective>Priest</adjective><member>Follower</member><prefix>true</prefix></li>
		  <li><ideoName>Temple of the Eight</ideoName><adjective>Priest</adjective><member>Follower</member><prefix>true</prefix></li>
		</symbolPacks>
		<fixedDeityNameTypes>
		  <li><name>Akatosh</name><type>the Dragon God of Time</type></li>
		  <li><name>Arkay</name><type>the God of Cycles</type></li>
		  <li><name>Dibella</name><type>the Godess of Beauty and Art</type></li>
		  <li><name>Julianos</name><type>the God of Logic</type></li>
		  <li><name>Kynareth</name><type>the Godess of heavens</type></li>
		  <li><name>Mara</name><type>the Mother-Godess</type></li>
		  <li><name>Stendarr</name><type>the God of Mercy</type></li>
		  <li><name>Zenithar</name><type>the Trader God</type></li>
		</fixedDeityNameTypes>
		<thingStyleCategories>

		</thingStyleCategories>
	</MemeDef>
	
